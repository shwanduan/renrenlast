<view class="fui-cell-group fui-shop-group" style="background-color:{{diyitem.style.background}};margin-top:{{diyitem.style.margintop}}rpx; margin-bottom:{{diyitem.style.marginbottom*2}}rpx">
    <navigator class="fui-list" openType="switchTab" url="/pages/index/index">
        <view class="fui-list-media">
            <image class="{{diyitem.params.logostyle}}" src="{{goods.shopdetail.logo}}"></image>
        </view>
        <view class="fui-list-inner">
            <view class="title" style="color:{{diyitem.style.shopnamecolor}}">{{goods.shopdetail.shopname}}<text class="title-wel" style="color:{{diyitem.style.rightnavcolor}}">{{goods.shopdetail.btntext2}}</text>
            </view>
            <view class="subtitle" style="color:{{diyitem.style.shopdesccolor}} " wx:if="{{goods.shopdetail.description}}">{{goods.shopdetail.description}}</view>
        </view>
    </navigator>
</view>
