<view class="fui-cell-group fui-cell-click" style="margin-top:0" wx:if="{{member.needbind==1}}">
    <navigator class="fui-cell" hoverClass="none">
        <view class="fui-cell-icon">
            <text class="icox icox-mobilephone"></text>
        </view>
        <view class="fui-cell-text">
            <button bindgetphonenumber="getPhoneNumber" class="text text-danger text_btn" lang="zh_CN" openType="getPhoneNumber">{{diyitem.params.title}}</button>
        </view>
        <view class="fui-cell-remark"></view>
    </navigator>
    <view class="fui-cell-tip">{{diyitem.params.text}}</view>
</view>
